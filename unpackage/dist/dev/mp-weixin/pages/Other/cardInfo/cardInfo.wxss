/* flex布局 */
.flex.data-v-2f0ef9b2, .flex-col.data-v-2f0ef9b2, .flex_be.data-v-2f0ef9b2, .flex_al-cen.data-v-2f0ef9b2, .flex_jus-cen.data-v-2f0ef9b2, .flex_ar.data-v-2f0ef9b2 {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.flex-col.data-v-2f0ef9b2 {
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
}
.flex_be.data-v-2f0ef9b2 {
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}
.flex_al-cen.data-v-2f0ef9b2 {
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.flex_jus-cen.data-v-2f0ef9b2 {
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
}
.flex_ar.data-v-2f0ef9b2 {
  -webkit-justify-content: space-around;
          justify-content: space-around;
}
.text_cen.data-v-2f0ef9b2 {
  text-align: center;
}
/* 省略号 */
.two-wrap.data-v-2f0ef9b2 {
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
